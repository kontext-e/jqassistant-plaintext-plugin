<jqa-plugin:jqassistant-plugin
        xmlns:jqa-plugin="http://www.buschmais.com/jqassistant/core/plugin/schema/v1.0" name="Plaintext">
    <description>Provides a scanner for plain text. Imports files line by line.</description>
    <model>
        <class>de.kontext_e.jqassistant.plugin.plaintext.store.descriptor.PlaintextDescriptor</class>
        <class>de.kontext_e.jqassistant.plugin.plaintext.store.descriptor.PlaintextFileDescriptor</class>
        <class>de.kontext_e.jqassistant.plugin.plaintext.store.descriptor.PlaintextLineDescriptor</class>
    </model>
    <scanner>
        <class>de.kontext_e.jqassistant.plugin.plaintext.scanner.PlaintextFileScannerPlugin</class>
    </scanner>
  <rules>
      <resource>plaintext.xml</resource>
  </rules>
</jqa-plugin:jqassistant-plugin>
